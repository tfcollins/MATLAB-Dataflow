
// MATLAB Datatype support functions
#include "RX_emxutil.h"

// Defines
#define NUMSAMPLES 18

//***Block Definitions***//

/////////////////////////////////////////////////
OUTPUTS CRCSource(INPUTS input, int *flag)
{
    // Setup output
    boi *OutSymbols = new double[NUMSAMPLES];
    
    // Process
    CICFilterABS(Symbols,OutSymbols);
            
    //Cleanup
    delete Symbols;

    //Enable pass to next block
    *flag = 1;
    return SendOut(OutSymbols);
}
/////////////////////////////////////////////////
OUTPUTS CRCSink(INPUTS input, int *flag)
{
    // Setup output
    double *OutSymbols = new double[NUMSAMPLES];
    
    // Get input
    creal_T *Symbols = (creal_T*) input[0];
    
    // Process
    CICFilterABS(Symbols,OutSymbols);
            
    //Cleanup
    delete Symbols;

    //Enable pass to next block
    *flag = 0;
    return SendOut(none);//none is a special variable for sinks
}
// [EOF]
